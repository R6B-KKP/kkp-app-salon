	<queryString>
		<![CDATA[select t.id transaksi_id, m.nama member, t.total_harga, t.tanggal, IF(t.id_voucher, t.id_voucher, "-") voucher, count(td.id_layanan) as layanan from transaksi t
join member m on m.id = t.id_member
join karyawan k on k.id = t.id_karyawan
join transaksi_detail td on td.id_transaksi = t.id 
where t.tanggal between $P{start_date} and $P{end_date}
group by t.id;]]>
	</queryString>
